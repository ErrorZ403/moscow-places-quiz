{
    "version": 2,
    "env": {
    },
    "builds": [
      {
        "src": "hook/src/index.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": [
            "app/public/**",
            "hook/src/places.json"
          ],
          "memory": 128
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "hook/src/index.ts"
      }
    ]
  }